  display: flex;\n}\n\n.btn {\n  border: none;\n  background-image: linear-gradient(to left bottom, #c31432, #240b36);\n  padding: 1rem 1.5rem;\n  color: #fff;\n  font-family: inherit;\n  text-transform: uppercase;\n  transition: all 0.2s;\n  cursor: pointer;\n  display: inline-block;\n  margin-right: 3rem;\n  font-size: 1.5rem;\n  outline: none;\n}\n@media only screen and (max-width: 37.5em) {\n  .btn {\n    margin-right: 2rem;\n  }\n}\n\n.btn:hover {\n  background-image: linear-gradient(to left bottom, rgba(195, 20, 50, 0.1), rgba(36, 11, 54, 0.1));\n  color: #c31432;\n  transform: translateY(-0.5rem);\n}\n\n.container {\n  max-width: 120rem;\n  margin: 0 auto;\n}\n\n.header {\n  height: 100vh;\n  background-size: cover;\n  background-attachment: fixed;\n  display: flex;\n  flex-direction: column;\n}\n\n.section {\n  padding: 4rem;\n  transition: transform 1s, opacity 1s;\n}\n\n.section-title {\n  text-align: center;\n  text-transform: uppercase;\n  margin: 8rem auto;\n}\n\n.section--hidden {\n  opacity: 0;\n  transform: translateY(8rem);\n}\n\n.nav__header {\n  padding: 2rem;\n  width: 100%;\n  z-index: 1000;\n}\n@media only screen and (max-width: 37.5em) {\n  .nav__header {\n    padding: 2rem 1rem;\n  }\n}\n.nav__links {\n  max-width: 120rem;\n  margin: 0 auto;\n  display: flex;\n  list-style: none;\n  justify-content: flex-end;\n  align-items: center;\n}\n@media only screen and (max-width: 37.5em) {\n  .nav__links {\n    justify-content: center;\n  }\n}\n.nav__links .icon {\n  height: 3rem;\n  width: 3rem;\n}\n.nav__link:link, .nav__link:visited {\n  font-size: 2rem;\n  text-decoration: none;\n  text-transform: uppercase;\n  padding: 0 1rem;\n  color: #fff;\n  fill: #fff;\n  transition: all 0.2s;\n}\n.nav__link:hover {\n  color: #ffa500;\n  fill: #ffa500;\n}\n\n.sticky {\n  position: fixed;\n  background-image: linear-gradient(to left bottom, rgba(195, 20, 50, 0.6), rgba(36, 11, 54, 0.6));\n}\n\n.header__text {\n  font-size: 8rem;\n  color: #fff;\n  font-weight